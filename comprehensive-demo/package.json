{
  "name": "comprehensive-demo",
  "scripts": {
    "start": "concurrently \"lerna run --scope=comprehensive-demo_app* --parallel start\" \"yarn serve\"",
    "build": "lerna run --scope=comprehensive-demo_app* build",
    "serve": "lerna run --scope=comprehensive-demo_app* --parallel serve",
    "clean": "lerna run --scope=comprehensive-demo_app* --parallel clean"
  },
  "devDependencies": {
    "concurrently": "7.3.0",
    "lerna": "3.22.1"
  }
}
