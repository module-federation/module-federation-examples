{
  "name": "react-ssr_shell",
  "version": "0.0.0",
  "scripts": {
    "start": "pnpm build && pnpm serve",
    "serve": "node dist/server/main.js",
    "build": "pnpm build:server && pnpm build:client",
    "build:client": "rimraf dist/client && webpack --mode production --progress --config config/webpack.client.js",
    "build:server": "rimraf dist/server && webpack --mode production --progress --config config/webpack.server.js"
  },
  "dependencies": {
    "express": "4.19.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "regenerator-runtime": "0.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/node": "7.24.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-react": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@module-federation/node": "2.5.20",
    "@types/express": "4.17.21",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-helmet": "6.1.11",
    "babel-loader": "9.1.3",
    "rimraf": "5.0.8",
    "webpack": "5.95.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "6.0.1"
  }
}