{
  "name": "@react-ssr/shell",
  "version": "0.0.0",
  "scripts": {
    "start": "yarn build && yarn serve",
    "serve": "node dist/server/main.js",
    "build": "yarn build:server && yarn build:client",
    "build:client": "rimraf dist/client && webpack --mode production --progress --config config/webpack.client.js",
    "build:server": "rimraf dist/server && webpack --mode production --progress --config config/webpack.server.js"
  },
  "dependencies": {
    "express": "4.18.2",
    "react": "16.6.3",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "react-version-adapter": "https://github.com/brunos3d/react-version-adapter.git",
    "regenerator-runtime": "0.13.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/node": "7.20.0",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.18.6",
    "@module-federation/node": "0.8.4",
    "@types/express": "4.17.14",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.17",
    "@types/react-helmet": "6.1.5",
    "babel-loader": "8.2.5",
    "rimraf": "3.0.2",
    "webpack": "5.73.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  }
}
