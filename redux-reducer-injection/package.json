    "serve": "lerna run --scope=redux-reducer-injection_app* --parallel serve",
    "clean": "lerna run --scope=redux-reducer-injection_app* --parallel clean"
  },
  "devDependencies": {
    "lerna": "3.22.1",
    "concurrently": "7.3.0"
  }
}
