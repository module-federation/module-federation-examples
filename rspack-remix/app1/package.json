  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@remix-run/dev": "2.8.1",
    "@rspack/cli": "0.6.3",
    "@rspack/core": "0.6.3",
    "@types/express": "4.17.21",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "babel-loader": "9.1.3",
    "babel-plugin-eliminator": "1.0.1",
    "esbuild": "0.19.11",
    "esbuild-loader": "4.1.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@remix-run/express": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "cors": "2.8.5",
    "express": "^4.18.2",
    "isbot": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}