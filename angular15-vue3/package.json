{
  "name": "angular15-vue3",
  "description": "This project shows an example of using Webpack 5 Module Federation with Angular `15` using Vue 3 components from remote.",
  "workspaces": [
    "vue-remote"
  ],
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:shell": "ng serve shell --live-reload false",
    "build:shell": "ng build shell",
    "start:remote": "npm --prefix ./vue-remote run serve",
    "e2e:test": "npx cypress run --config-file ../cypress-e2e/config/cypress.config.ts --config '{\"supportFile\": \"../cypress-e2e/support/e2e.ts\"}' --spec \"./e2e/*.cy.ts\" --browser=chrome",
    "e2e:ci": "node e2e.ci.js"
  },
  "dependencies": {
    "@angular/animations": "15.0.4",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/router": "15.0.4",
    "@ngxs/store": "3.8.1",
    "bootstrap": "^4.5.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.1.0",
    "@angular-devkit/build-angular": "15.2.10",
    "@angular/cli": "14.2.13",
    "@angular/compiler-cli": "15.0.4",
    "@types/jasmine": "4.6.4",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "12.20.55",
    "codelyzer": "6.0.2",
    "concurrently": "7.6.0",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "15.2.2",
    "ngx-build-plus": "15.0.0",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.8.4",
    "wait-on": "7.2.0"
  }
}
