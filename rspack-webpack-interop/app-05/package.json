{
  "name": "rspack_webpack_app-05",
  "version": "1.0.0",
  "description": "boilerplate LitElement setup with TypeScript",
  "scripts": {
    "build": "cross-env NODE_ENV=production rspack build --mode production",
    "legacy:build": "cross-env NODE_ENV=production webpack",
    "start": "rspack serve",
    "legacy:start": "webpack --watch",
    "dev": "rspack serve",
    "legacy:dev": "webpack-dev-server --content-base public",
    "serve": "serve public -p 3005",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "lit-element": "2.5.1",
    "ts-loader": "5.4.5",
    "typescript": "4.3.5",
    "html-webpack-plugin": "5.6.0",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "serve": "14.2.1",
    "@rspack/core": "^0.4.5",
    "@rspack/cli": "^0.4.5",
    "@rspack/dev-server": "^0.4.5"
  }
}
