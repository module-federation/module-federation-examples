{
  "name": "rollup-federation-demo_rollup-spa-systemjs",
  "version": "1.0.0",
  "main": "dist",
  "license": "MIT",
  "scripts": {
    "start": "concurrently 'pnpm watch' 'pnpm serve'",
    "build": "cross-env NODE_ENV=development rollup -c",
    "serve": "serve -d . -p 8082",
    "watch": "cross-env NODE_ENV=development rollup -c -w",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@module-federation/rollup-federation": "0.0.1",
    "@rollup/plugin-commonjs": "13.0.2",
    "@rollup/plugin-node-resolve": "8.4.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.79.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-inject-process-env": "1.3.1",
    "serve": "13.0.4"
  }
}
