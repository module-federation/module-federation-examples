    "@module-federation/node": "^2.2.3",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "babel-loader": "8.2.5",
    "html-webpack-plugin": "5.6.0",
    "rimraf": "3.0.2",
    "webpack": "^5.91.0",
