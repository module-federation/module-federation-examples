    "@module-federation/node": "2.2.14",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "babel-loader": "9.1.3",
    "html-webpack-plugin": "5.6.0",
    "rimraf": "5.0.7",
    "webpack": "5.92.1",
