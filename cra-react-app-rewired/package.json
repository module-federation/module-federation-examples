{
  "name": "cra-react-app-rewired",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "postinstall": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +; pnpm install --ignore-scripts",
    "start": "pnpm --filter cra-react-app-rewired_* --parallel start",
    "build": "pnpm --filter cra-react-app-rewired_* --parallel build",
    "serve": "pnpm --filter cra-react-app-rewired_* --parallel serve",
    "clean": "pnpm --filter cra-react-app-rewired_* --parallel clean",
    "e2e:ci": "pnpm start & sleep 30 && echo 'test' && npx cypress run --config-file ../cypress-e2e/config/cypress.config.ts --config '{\"supportFile\": \"../cypress-e2e/support/e2e.ts\"}' --spec \"./e2e/tests/*.cy.ts\" --browser=chrome"
  },
  "devDependencies": {
    "wait-on": "7.2.0"
  }
}
